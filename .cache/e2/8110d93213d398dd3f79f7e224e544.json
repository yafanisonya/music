\n    width:200vw;\r\n    display: flex;\r\n\r\n    transition: transform .3s;\r\n    align-items: center;\r\n\r\n    &.panel1{\r\n      transform: translateX(0vw);\r\n    }\r\n    &.panel2{\r\n      transform: translateX(-100vw);\r\n    }\r\n    .panel-effect{\r\n      display: flex;\r\n      width: 100vw;\r\n      height: 100%;\r\n      flex-direction: column;\r\n      justify-content: center;\r\n      .effect{\r\n        position: relative;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        height: 40vh;\r\n        >div{\r\n          background-size: contain;\r\n          position: absolute;\r\n        }\r\n        .effect-1{\r\n          background:url(\"../svg/effect-no-move.svg\") 0 0 no-repeat;\r\n          width: 70vw;\r\n          height: 70vw;\r\n          animation:rotate 15s linear infinite;\r\n        }\r\n        .effect-2{\r\n          background:url(\"../svg/effect-move1.svg\") 0 0 no-repeat;\r\n          width: 60vw;\r\n          height: 60vw;\r\n          animation: rotate 10s linear reverse;\r\n        }\r\n        .effect-3{\r\n          background: url(\"../svg/effect-move2.svg\") 0 0 no-repeat;\r\n          width:24vw;\r\n          height:24vw;\r\n          animation: rotate 5s linear infinite;\r\n        }\r\n      }\r\n      .lyrics{\r\n        text-align: center;\r\n        p{\r\n          font-size:14px;\r\n          color:$color-default;\r\n          margin-top: 8px;\r\n          &.current{\r\n            color:$color;\r\n          }\r\n        }\r\n      }\r\n    }\r\n    .panel-lyrics{\r\n      width:100vw;\r\n      height:100%;\r\n    }\r\n  }\r\n  .footer{\r\n    height: 160px;\r\n    .buttons{\r\n      display: flex;\r\n      justify-content: space-between;\r\n      svg{\r\n        color:$color-default;\r\n        width:24px;\r\n        height: 24px;\r\n        margin: 0 20px;\r\n      }\r\n    }\r\n    .area-bar{\r\n      color:$color-default;\r\n      font-size: 12px;\r\n      padding:0 20px;\r\n      margin-top:30px;\r\n      display: flex;\r\n      align-items: center;\r\n      .time-start{ order: 1;}\r\n      .time-end{ order: 3;}\r\n      .bar{\r\n        order:2;\r\n        flex-grow: 1;\r\n        height:4px;\r\n        background:$color-unuse;\r\n        border-radius: 2px;\r\n        margin:0 10px;\r\n        .progress{\r\n          width:20%;\r\n          height:100%;\r\n          border-radius: 2px;\r\n          background: $color-use;\r\n          position:relative;\r\n          &::after{\r\n            content: '';\r\n            position:absolute;\r\n            right:0;\r\n            top:-7px;\r\n            display: block;\r\n            width:16px;\r\n            height:16px;\r\n            background: url(\"../svg/progress.svg\") 0 0 no-repeat;\r\n            background-size:contain;\r\n          }\r\n        }\r\n      }\r\n    }\r\n    .actions{\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: space-between;\r\n      margin-top:25px;\r\n      .icon{\r\n        color:$color-default;\r\n        width:36px;\r\n        height: 36px;\r\n        margin:0 20px;\r\n      }\r\n    }\r\n  }\r\n}\r\n@keyframes rotate {\r\n  0%{transform: rotate(0deg);}\r\n  100%{transform: rotate(360deg)}\r\n}"},"lineCount":null}},"error":null,"hash":"3bc3175e1f32a1fa812beac4f9ef775f","cacheData":{"env":{}}}